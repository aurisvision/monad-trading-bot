{
  "name": "area51-telegram-bot",
  "version": "1.0.0",
  "description": "Area51 - Telegram Trading Bot for Monad Testnet",
  "main": "src/index-scalable.js",
  "scripts": {
    "start": "node src/index-scalable.js",
    "start:scalable": "node src/index-scalable.js",
    "start:cluster": "node src/cluster.js",
    "dev": "nodemon src/index-scalable.js",
    "migrate": "node scripts/migrate-to-postgresql.js",
    "test-migration": "node scripts/test-migration.js",
    "backup": "node backup/database-backup.js",
    "health": "curl -f http://localhost:3001/health || exit 1",
    "cleanup": "node -e \"require('./src/database-postgresql').cleanup()\"",
    "test": "node scripts/test-migration.js",
    "start:production": "node production-start.js",
    "start:single": "NODE_ENV=production node src/index-scalable.js"
  },
  "keywords": [
    "telegram",
    "bot",
    "trading",
    "monad",
    "defi"
  ],
  "author": "Area51 Team",
  "license": "MIT",
  "dependencies": {
    "abort-controller": "^3.0.0",
    "axios": "^1.6.2",
    "bip39": "^3.1.0",
    "cluster": "^0.7.7",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "ethers": "^6.8.1",
    "express": "^4.21.2",
    "ioredis": "^5.7.0",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "redis": "^5.8.2",
    "telegraf": "^4.15.6",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
